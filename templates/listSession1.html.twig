{% if sessions %}
	<div class="row  ">
<div class="accordion" id="accordionExample">
  <div class="accordion-item">
    <h2 class="accordion-header" id="headingOne">
      <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
      <h4 class="txt-dark">Apprenti</h4>
      </button>
    </h2>
    <div id="collapseOne" class="accordion-collapse collapse show" aria-labelledby="headingOne" data-bs-parent="#accordionExample">
      <div class="accordion-body">
       <div class="row bg">
									<div class="col-12">
											{# XXXXXXXXXXXXXXXXXX  attention all role <a href="{{ path( 'addUser', {'role':1}) }}"> #}

											{% if is_granted('ROLE_OF') %}
												<a href="{{ path( 'addUser',{'role': 'ROLE_APP','roleName': 'ROLE_APP'} ) }}">
													<div class="boutonForm">+ Créer un Apprenti</div>
												</a>

												{% else %}
												{# <a href="{{ path( 'addUser',{'role': 'ROLE_APP','roleName': 'ROLE_APP'} ) }}">
																																																	<div class="boutonForm">+ Créer un Apprenti</div>
																																																</a> #}
											{% endif %}
										</div>
									</div>
									<div class="row">
										<div class="col-12">
											<table class="table-normal" class=" colorTextNoir">
												<tr>
													<th class=" colorTextNoir">Prenom</th>
													<th class=" colorTextNoir">nom</th>
													{# <th class = " colorTextNoir">session</th> #}
													<th class=" colorTextNoir">Tél</th>
													<th class=" colorTextNoir">Mail</th>
												</tr>
												<tbody>
													{% for elme in listAPPS[i] %}
                                                 
                                                        
                                                    
														<tr
															class='clickable-row' data-href='{{ path( 'editUser',{'user':elme.id} ) }}'>
															{# <a href="{{ path( 'editUser',{'user':item.id} ) }}"> #}
															<td class=" colorTextNoir">{{elme.prenom}}</td>
															<td class=" colorTextNoir">{{elme.nom}}</td>
															<td class=" colorTextNoir">{{elme.telephone}}</td>
															<td class=" colorTextNoir">{{elme.email}}</td>
														</tr>
                                                 
													{% endfor %}
												</tbody>
											</table>
										</div>
									</div>
								</div>
      </div>
    </div>
  </div>
