{% if sessions %}
	<div class="row  ">
		<div class="accordion" id="accordionExample">
  			<div class="accordion-item">
    				<h2>
        		<div class="mt-3 text-center" style=" border: 2px #11284B!important; background-color: #e7f1ff; box-shadow: inset 0 -1px 0 rgb(0 0 0 / 13%);">
   					<h4 class="txt-dark">Apprenti</h4>
      			</div>
    		</h2>
    		<div>
      			<div >
        			<div class="row bg">
							<div class="col-12">
								{% if is_granted('ROLE_OF') %}
									<a href="{{ path( 'addUser',{'role': 'ROLE_APP','roleName': 'ROLE_APP'} ) }}">
										<div class="boutonForm">+ Créer un Apprenti</div>
									</a>
								{% else %}
								 {# <a href="{{ path( 'addUser',{'role': 'ROLE_APP','roleName': 'ROLE_APP'} ) }}">
										<div class="boutonForm">+ Créer un Apprenti</div>
									</a> #}
								{% endif %}
							</div>
						</div>
						<div class="row">
							<div class="col-12">
								<table class="table-normal  txt-normal">
									<tr class=" bg-bar">
										<th class=" col-3">Prénom</th>
										<th class=" col-3">Nom</th>
									 {# <th class = " colorTextNoir">session</th> #}
										<th class=" col-3">Tél</th>
										<th class=" col-3">Mail</th>
									</tr>
								<tbody>
									{% for elme in listAPPS[i] %}
                                    <tr	class='clickable-row' data-href='{{ path( 'profilOF_APP',{'user':elme.id} ) }}'>
									 {# <a href="{{ path( 'editUser',{'user':item.id} ) }}"> #}
										<td class=" colorTextNoir">{{elme.prenom}}</td>
										<td class=" colorTextNoir">{{elme.nom}}</td>
										<td class=" colorTextNoir">{{elme.telephone}}</td>
										<td class=" colorTextNoir"><A HREF="mailto:{{elme.email}}">{{elme.email}}</A></td>
									</tr>
									{% endfor %}
								</tbody>
								</table>
							</div>
						</div>
					</div>
        		</div>
    		</div>
  		</div>
  		<div>
   			<h2>
        		<div class="mt-3 text-center" style=" color: #0c63e4; background-color: #e7f1ff; box-shadow: inset 0 -1px 0 rgb(0 0 0 / 13%);">
